[
  {
    "name": "h8s-horreum",
    "image": "stopa323/h8s-horreum:0.1.0.build.26",
    "networkMode": "awsvpc",
    "portMappings": [
      {
        "containerPort": 8003
      }
    ],
    "environment": [
      {
        "name": "DYNAMODB_REGION",
        "value": "eu-west-1"
      }
    ],
    "command": [
      "--host", "0.0.0.0",
      "--port", "8003"
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "h8s",
        "awslogs-region": "eu-west-1",
        "awslogs-stream-prefix": "horreum"
      }
    }
  }
]
